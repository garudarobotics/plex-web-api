      </span><span class="s2">"name"</span><span class="p">:</span><span class="w"> </span><span class="s2">"Rugada Robotics Pte. Ltd."</span><span class="p">,</span><span class="w">
      </span><span class="s2">"drone_provider_id"</span><span class="p">:</span><span class="w"> </span><span class="s2">"fda3963d33db33c5023d33b5e863a19f"</span><span class="p">,</span><span class="w">
      </span><span class="s2">"brand"</span><span class="p">:</span><span class="w"> </span><span class="s2">"Rugada"</span><span class="p">,</span><span class="w">
