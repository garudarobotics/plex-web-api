          </span><span class="s2">"conditionYawTarget"</span><span class="p">:</span><span class="w"> </span><span class="p">{</span><span class="w"> 
              </span><span class="s2">"lat"</span><span class="p">:</span><span class="w"> </span><span class="s2">"2.0"</span><span class="p">,</span><span class="w">
